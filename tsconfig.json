{
  "compilerOptions": {
    "declaration": false,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "lib": ["es2022"],
    "target": "es2022",
    "module": "es2022",
    "typeRoots": ["./node_modules/@types"],
    "noUnusedParameters": true,
    "exactOptionalPropertyTypes": true
  },
  "include": ["./type/", "./build/", "./lib/", "./test/", "./lint/", "yo.mjs"],
  "exclude": ["./test/262/codebase/"]
}
