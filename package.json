{
  "name": "aran",
  "description": "Aran is a JavaScript instrumenter for building dynamic analysis tools",
  "version": "3.0.9",
  "author": {
    "name": "Laurent Christophe",
    "email": "lachrist@vub.ac.be"
  },
  "main": "lib/main.js",
  "type": "module",
  "repository": "lachrist/aran",
  "homepage": "http://github.com/lachrist/aran",
  "license": "MIT",
  "scripts": {
    "lint": "npx tsc && npx prettier -c '**/*.(mjs|d.ts)' && npx eslint 'lint/**/*.mjs' 'test/**/*.mjs' 'lib/**/*.mjs'",
    "format": "npx prettier -w '**/*.(mjs|d.ts)'",
    "test": "node test/test.mjs",
    "coverage": "node test/coverage.mjs",
    "load-test262": "git clone --filter=tree:0 https://github.com/tc39/test262",
    "test262-batch": "node --experimental-vm-modules --expose-gc test/262/batch.mjs",
    "x": "node --experimental-vm-modules --expose-gc test/262/progress.mjs test/262/cursor.json",
    "xx": "node --experimental-vm-modules --expose-gc test/262/single.mjs test/262/cursor.json"
  },
  "keywords": [
    "JavaScript",
    "Instrumentation",
    "Dynamic Analysis",
    "Sandboxing",
    "Monitoring",
    "Tracing",
    "Shadow Execution"
  ],
  "devDependencies": {
    "@babel/generator": "^7.22.15",
    "@babel/parser": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@bercow/core": "^0.2.0",
    "@bercow/eslint": "^0.1.0",
    "@bercow/link-adjacent": "^0.1.0",
    "@bercow/order-esm-import": "^0.1.0",
    "@bercow/prettier": "^0.1.0",
    "@bercow/spawn": "^0.2.0",
    "@types/eslint": "^8.44.3",
    "@types/estree": "^1.0.1",
    "@types/node": "^20.4.4",
    "@typescript-eslint/parser": "^6.7.4",
    "acorn": "^8.11.2",
    "astring": "^1.8.6",
    "c8": "^7.10.0",
    "chalk": "^4.1.2",
    "diff": "^5.0.0",
    "escodegen": "^2.1.0",
    "eslint": "^8.51.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-local": "^1.0.0",
    "pegjs": "^0.10.0",
    "prettier": "^3.0.0",
    "typescript": "^5.4.5",
    "yaml": "^2.3.2"
  }
}
