{
  "name": "aran",
  "description": "Aran is a JavaScript instrumenter for building dynamic analysis tools",
  "version": "3.0.9",
  "author": {
    "name": "Laurent Christophe",
    "email": "lachrist@vub.ac.be"
  },
  "main": "lib/main.js",
  "repository": "lachrist/aran",
  "homepage": "http://github.com/lachrist/aran",
  "license": "MIT",
  "scripts": {
    "build": "sh lib/lang/parse/deploy.sh",
    "lint": "npx eslint '**/*.mjs'",
    "unit": "node ./node_modules/.bin/test-turtle --layout alongside 'npx prettier -w $1 $2 && npx eslint --rulesdir eslint-rules $1 $2 && node $2 && npx c8 --include $1 --check-coverage --branches 100 --functions 100 -- node $2'"
  },
  "keywords": [
    "JavaScript",
    "Instrumentation",
    "Dynamic Analysis",
    "Sandboxing",
    "Monitoring",
    "Tracing",
    "Shadow Execution"
  ],
  "devDependencies": {
    "acorn": "^8.6.0",
    "c8": "^7.10.0",
    "chalk": "^4.1.2",
    "diff": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "pegjs": "^0.10.0",
    "prettier": "^2.4.1",
    "test-turtle": "^4.0.2"
  },
  "dependencies": {
    "array-lite": "^0.1.9"
  }
}
